{
    "env": {
        "browser": true
    },
    "extends": [
        "@react-native-community",
        "google"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "prettier/prettier": 0,
        "require-jsdoc": 0,
        "max-len": 0,
        "linebreak-style": 0,
        "new-cap": 0
    }
}
